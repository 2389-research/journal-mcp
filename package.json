{
  "name": "private-journal-mcp",
  "version": "1.2.0",
  "description": "A comprehensive MCP server that provides Claude with private journaling, semantic search, resources, and guided prompts for reflection and learning",
  "main": "dist/index.js",
  "bin": {
    "private-journal-mcp": "./dist/index.js"
  },
  "scripts": {
    "prepare": "npm run build",
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "mcp",
    "journal",
    "private",
    "claude",
    "ai"
  ],
  "author": "Jesse Vincent <jesse@fsck.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "prettier": "^3.0.0",
    "supertest": "^7.1.1",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.4.0",
    "@types/node-fetch": "^2.6.12",
    "@xenova/transformers": "^2.17.2",
    "node-fetch": "^3.3.2"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ]
}
